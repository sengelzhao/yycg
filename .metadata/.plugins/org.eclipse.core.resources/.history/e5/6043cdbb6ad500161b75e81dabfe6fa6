public class Touzihuibao {	
	public static void main(String[] args) throws Exception {
                 
           Touzihuibao t = new Touzihuibao();
           t.shouYi(300, 2,1);
		
		
	}
	
	public void shouYi(int n,double rate,int money){

		    double sum=1;
	
			for(int i=1;i<=n;i++)
			{
				sum*=rate;
				System.out.println(sum);
			}
		
         
		
			
		
	}
	
	
}
	  
	 



